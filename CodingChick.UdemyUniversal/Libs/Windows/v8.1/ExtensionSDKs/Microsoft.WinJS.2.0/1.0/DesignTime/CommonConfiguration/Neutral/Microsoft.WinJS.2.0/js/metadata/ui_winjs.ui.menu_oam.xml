<?xml version="1.0" encoding="utf-8"?>
<widget version="1.0" spec="1.0" id="ms-appx://Microsoft.WinJS.2.0/js/ui/WinJS.UI.Menu" name="WinJS.UI.Menu" xmlns:blend="http://schemas.microsoft.com/winjs/Blend/2011" blend:supportsContent="true" xmlns="http://openajax.org/metadata">
  <author name="Microsoft Corporation" />
  <title locid="WinJS.UI.Menu_name" type="text/plain">Menu</title>
  <description locid="WinJS.UI.Menu" type="text/plain">Represents a menu flyout for displaying commands.</description>
  <blend:compatibleWiths>
    <blend:compatibleWith platform="Windows" minVersion="8.0" />
  </blend:compatibleWiths>
  <icons>
    <icon src="ui_winjs.ui.menu.12x12.png" />
    <icon src="ui_winjs.ui.menu.16x16.png" />
  </icons>
  <properties>
    <property name="commands" datatype="Array" required="false" blend:isAdvanced="true">
      <description locid="WinJS.UI.Menu.commands" type="text/plain">Sets the MenuCommand objects that appear in the Menu. You can set this to a single MenuCommand or an array of MenuCommand objects.</description>
      <blend:compatibleWiths>
        <blend:compatibleWith platform="Windows" minVersion="8.0" />
      </blend:compatibleWiths>
    </property>
  </properties>
  <content><![CDATA[<div data-win-control="WinJS.UI.Menu">
 <button data-win-control="WinJS.UI.MenuCommand" data-win-options="{id:'',label:'example',type:'button',onclick:null}"></button>
 </div>]]></content>
  <requires>
    <require type="javascript" src="//Microsoft.WinJS.2.0/js/base.js" includeRef="true" p4:distinct="false" xmlns:p4="http://schemas.microsoft.com/winjs/2011" />
    <require type="javascript" src="//Microsoft.WinJS.2.0/js/ui.js" includeRef="true" p4:distinct="false" xmlns:p4="http://schemas.microsoft.com/winjs/2011" />
    <require type="css" src="//Microsoft.WinJS.2.0/css/ui-dark.css" includeRef="true" p4:distinct="false" xmlns:p4="http://schemas.microsoft.com/winjs/2011" />
    <require type="javascript"><![CDATA[WinJS.UI.processAll();]]></require>
  </requires>
  <blend:parts>
    <part name="menu" class="win-menu">
      <description locid="WinJS.UI.Menu_part:menu" type="text/plain">The Menu control itself</description>
    </part>
  </blend:parts><locale messages="ui_loc_oam.xml" />
</widget>