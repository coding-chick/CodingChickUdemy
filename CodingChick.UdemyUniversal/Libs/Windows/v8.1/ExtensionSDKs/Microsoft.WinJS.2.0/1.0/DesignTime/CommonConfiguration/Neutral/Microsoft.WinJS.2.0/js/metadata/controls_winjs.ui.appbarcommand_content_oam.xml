<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://openajax.org/metadata"
        xmlns:blend="http://schemas.microsoft.com/winjs/Blend/2011"
        spec="1.0"
        id="http://www.microsoft.com/schemas/corsica/1.0/AppBarCommand_Content"
        name="WinJS.UI.AppBarCommand_Content"
            blend:supportsContent="true"
        version="1.0">
  <author name="Microsoft Corporation" />
  <title type="text/plain">AppBar Command Content</title>
  <icons>
    <icon src="controls_winjs.ui.appbarcommand_content.12x12.png" width="12" height="12" />
    <icon src="controls_winjs.ui.appbarcommand_content.16x16.png" width="16" height="16" />
  </icons>
  <description type="text/plain">
    <![CDATA[Command container hosts arbitrary content inside of AppBar with commands layout.]]>
  </description>
  <requires>
    <require type="javascript" src="//Microsoft.WinJS.2.0/js/base.js" />
    <require type="javascript" src="//Microsoft.WinJS.2.0/js/ui.js" />
    <require type="javascript"><![CDATA[WinJS.UI.processAll();]]></require>
  </requires>
  <content>
    <![CDATA[
        <div data-win-control="WinJS.UI.AppBarCommand" data-win-options="{type:'content'}"></div>
    ]]>
  </content>
</widget>

